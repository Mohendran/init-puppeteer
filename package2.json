{
  "name": "tag-fn",
  "version": "0.2.6",
  "description": "CLI command for lazy generation of Github tags",
  "main": "dist/index.ts",
  "typings": "index.d.ts",
  "repository": "git@github.com:selfrefactor/tag-fn.git",
  "author": "Dejan Toteff <deyan8284@gmail.com>",
  "license": "GPL-3.0",
  "private": false,
  "dependencies": {
    "fs-extra": "^4.0.2",
    "inquirer": "^3.3.0",
    "log-fn": "^2.1.0",
    "puppeteer": "0.11.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^4.0.2",
    "@types/jest": "^21.1.2",
    "@types/node": "^8.0.33",
    "@types/puppeteer": "https://github.com/selfrefactor/puppeteer-types#0.4.0",
    "env-fn": "^1.1.1",
    "jest": "^21.2.1",
    "ts-jest": "^21.1.0",
    "tslint": "^5.7.0",
    "typescript": "^2.6.0-dev.20171007",
    "watch-fn": "^0.6.1"
  },
  "scripts": {
    "dev": "jest __tests__ --watch",
    "start": "cd files&&./start.sh",
    "test": "jest",
    "cover": "jest __tests__ --coverage",
    "build": "tsc -p .",
    "lint": "tslint --type-check --project tsconfig.json"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ]
  },
  "preferGlobal": true,
  "bin": {
    "tag": "bin/tag"
  },
  "directories": {
    "bin": "./bin"
  }
}
