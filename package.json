		"lint": "tslint",
		"format": "tsfmt",
		"test": "jest"
	},
	"private": false,
	"dependencies": {
		"client-helpers": "https://github.com/selfrefactor/client-helpers#0.5.0",
		"rambdax": "^0.8.10"
	},
	"devDependencies": {
		"@types/jest": "^22.1.4",
		"@types/node": "^9.4.6",
		"@types/puppeteer": "^1.0.1",
		"jest": "^22.4.2",
		"puppeteer": "^1.1.1",
		"ts-jest": "^22.4.1",
		"tslint": "^5.9.1",
		"typescript": "^2.8.0-insiders.20180127",
		"typescript-formatter": "^7.1.0",
		"watch-fn": "https://github.com/selfrefactor/watch-fn#0.8.1"
	},
	"jest": {
		"transform": {
			"^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
		},
		"testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
